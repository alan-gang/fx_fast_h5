.app-header-view {
  height 88px
  background $app-header-bg-color
  .more
  .setting
  .refresh {
    width 44px
    height 44px
    background-image url('~src/assets/images/default/nav-more@2x.png')
    background-repeat no-repeat
    background-position center
    background-size 44px
  }
  .setting {
    background-image url('~src/assets/images/default/nav-set@2x.png')
  }
  .refresh {
    width 24px
    height 24px
    background-image url('~src/assets/images/default/nav-ref@2x.png')
    background-size 24px
  }
}
